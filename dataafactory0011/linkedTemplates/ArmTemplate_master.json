{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dataafactory0011"
		},
		"datalake00111_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake00111'"
		},
		"dataworkspace0011_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dataworkspace0011'"
		},
		"datalake00111_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake00111.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dataafactory0011_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake00111_accountKey": {
						"value": "[parameters('datalake00111_accountKey')]"
					},
					"dataworkspace0011_connectionString": {
						"value": "[parameters('dataworkspace0011_connectionString')]"
					},
					"datalake00111_properties_typeProperties_url": {
						"value": "[parameters('datalake00111_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}